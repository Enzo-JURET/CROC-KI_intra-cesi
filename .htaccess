# Turn rewrite engine on 
Options +FollowSymlinks
RewriteEngine on

# Map neat URL to internal URL 
# Fichier qui va faire le lien entre l'url rentré et le controller utilisé

RewriteRule ^data/(amis|authentification|getSomething|modification_profil|fil_actualite|ajout_actualite|supprimerAmi|envoyerDemandeAmi|choixReponseDemandeAmi|etat_connexion|creerGroupe)(/|) controller/restcontroller.php?type=$1 [nc,qsa]
